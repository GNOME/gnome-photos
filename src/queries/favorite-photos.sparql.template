SELECT {{projection}}
{
    ?urn a nmm:Photo .
    ?urn a nmm:Photo; nao:hasTag nao:predefined-tag-favorite .
    OPTIONAL { ?urn nco:creator ?creator . }
    OPTIONAL { ?urn nco:publisher ?publisher . }
    {{item_pattern}}
    FILTER ({{photos_default_filter}} && {{source_filter}} && {{search_filter}})
}
GROUP BY ?urn
{{order}}
{{offset_limit}}
